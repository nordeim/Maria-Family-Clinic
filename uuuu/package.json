    "@trpc/server": "10.45.2",
    "@vis.gl/react-google-maps": "^1.7.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.451.0",
    "next": "14.2.22",
    "next-auth": "4.24.8",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-hook-form": "^7.53.1",
    "recharts": "^2.12.7",
    "sonner": "^1.7.1",
