{
  "name": "my-family-clinic",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "prepare": "husky install",
    "test:compliance": "tsx testing/compliance/test-runner.ts",
    "test:compliance:full": "tsx testing/compliance/test-runner.ts --full",
    "test:compliance:pdpa": "tsx testing/compliance/test-runner.ts --category PDPA",
    "test:compliance:moh": "tsx testing/compliance/test-runner.ts --category MOH",
    "test:compliance:security": "tsx testing/compliance/test-runner.ts --category SECURITY",
    "test:compliance:privacy": "tsx testing/compliance/test-runner.ts --category PRIVACY",
    "test:compliance:vulnerability": "tsx testing/compliance/test-runner.ts --no-vulnerability",
    "test:compliance:report": "tsx testing/compliance/test-runner.ts --output html",
    "test:performance": "tsx testing/performance/TestRunner.ts",
    "test:performance:emergency": "tsx testing/performance/TestRunner.ts --suite emergency --env staging",
    "test:performance:appointment": "tsx testing/performance/TestRunner.ts --suite appointment --env staging",
    "test:performance:enrollment": "tsx testing/performance/TestRunner.ts --suite enrollment --env staging",
    "test:performance:scalability": "tsx testing/performance/TestRunner.ts --suite scalability --env staging",
    "test:performance:comprehensive": "tsx testing/performance/TestRunner.ts --suite comprehensive --parallel",
    "test:performance:continuous": "tsx testing/performance/TestRunner.ts --suite emergency --continuous --interval 300",
    "test:performance:load-test": "tsx testing/performance/load-testing/HealthcareLoadTestFramework.ts",
    "test:performance:core-web-vitals": "tsx testing/performance/core-web-vitals/CoreWebVitalsValidation.ts",
    "test:performance:cross-platform": "tsx testing/performance/cross-platform/CrossPlatformPerformanceTest.ts",
    "test:performance:healthcare": "tsx testing/performance/healthcare-specific/HealthcarePerformanceTest.ts",
    "test:performance:scalability-test": "tsx testing/performance/scalability/ScalabilityTest.ts"
  },
  "dependencies": {
