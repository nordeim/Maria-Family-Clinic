// Routing Rules for Multi-Channel
model RoutingRule {
  id                String   @id @default(cuid())
  name              String   @unique
  description       String?  @db.Text
  
  // Rule configuration
  isActive          Boolean  @default(true)
  priority          Int      @default(0) // Lower number = higher priority
  channelId         String?  // Specific channel or null for all
  
  // Conditions
  conditions        Json     @default("[]") // Array of conditions
  logicOperator     LogicOperator @default(AND)
  
  // Actions
  targetType        RoutingTargetType
  targetId          String?  // Agent, team, or department ID
  targetName        String
  action            String?  // Action to perform
  
  // Load balancing
  method            RoutingMethod @default(DIRECT)
  roundRobinGroup   String?  // For round-robin routing
  maxWorkload       Int?     // Maximum workload
  availabilityCheck Boolean  @default(true)
  
  // Timing
  businessHoursOnly Boolean  @default(false)
  effectiveHours    Json     @default("{}")
  timezone          String?  // Rule timezone
  
  // Fallback
  fallbackEnabled   Boolean  @default(false)
  fallbackRuleId    String?  // Fallback routing rule
  
  // Performance tracking
  usageCount        Int      @default(0)
  successRate       Float?   // Success rate
  avgResponseTime   Int?     // Average response time
  
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt

  channel           CommunicationChannel? @relation(fields: [channelId], references: [id], onDelete: SetNull)

  @@index([isActive])
  @@index([priority])
  @@index([channelId])
  @@index([targetType, targetId])
  @@map("routing_rules")
}

// ========================================
// ENUMS FOR MULTI-CHANNEL SYSTEM
// ========================================

// Channel Types
enum ChannelType {
  EMAIL
  SMS
  PHONE
  CHAT
  WHATSAPP
  TELEGRAM
  FACEBOOK
  TWITTER
  INSTAGRAM
  LINKEDIN
  WEB_FORM
  VIDEO_CALL
  VOICE_MESSAGE
  SOCIAL_MEDIA
  MOBILE_APP
  PORTAL
  API
}

// Channel Categories
enum ChannelCategory {
  SYNCHRONOUS    // Real-time communication
  ASYNCHRONOUS   // Non-real-time communication
  SOCIAL         // Social media platforms
  BUSINESS       // Business communication tools
  EMERGENCY      // Emergency communication channels
  ACCESSIBILITY  // Accessibility-focused channels
}

// Integration Types
enum IntegrationType {
  INBOUND        // Incoming message integration
  OUTBOUND       // Outgoing message integration
  BIDIRECTIONAL  // Two-way communication
  WEBHOOK        // Webhook-based integration
  API            // API-based integration
  SOCIAL         // Social media integration
  VOICE          // Voice services integration
  VIDEO          // Video conferencing integration
}

// Integration Health Status
enum IntegrationHealth {
  HEALTHY
  WARNING
  CRITICAL
  DOWN
  UNKNOWN
  MAINTENANCE
}

// Conversation Status
enum ConversationStatus {
  ACTIVE
  WAITING
  PENDING
  ON_HOLD
  ESCALATED
  RESOLVED
  CLOSED
  ARCHIVED
  CANCELLED
}

// Conversation Priority
enum ConversationPriority {
  LOW
  NORMAL
  HIGH
  URGENT
  CRITICAL
  VIP
  EMERGENCY
}

// Message Types
enum MessageType {
  TEXT
  IMAGE
  DOCUMENT
  AUDIO
  VIDEO
  LOCATION
  CONTACT
  STICKER
  TEMPLATE
  SYSTEM
  NOTIFICATION
  CAROUSEL
  QUICK_REPLY
  INTERACTIVE
  FORM
}

// Media Types
enum MediaType {
  IMAGE
  VIDEO
  AUDIO
  DOCUMENT
  APPLICATION
  OTHER
}

// Message Sender Types
enum MessageSenderType {
  CUSTOMER
  AGENT
  BOT
  SYSTEM
  AUTOMATION
  WEBHOOK
  API
}

// Message Recipient Types
enum MessageRecipientType {
  CUSTOMER
  AGENT
  GROUP
  BROADCAST
  SYSTEM
  API
}

// Message Status
enum MessageStatus {
  PENDING
  SENDING
