// ========================================
// MULTI-CHANNEL CONTACT INTEGRATION SYSTEM
// Sub-Phase 9.7: Comprehensive Multi-Channel Communication Platform
// Extends existing contact system with unified multi-channel support
// ========================================

// Communication Channel Configuration
model CommunicationChannel {
  id                String   @id @default(cuid())
  name              String   @unique // 'phone', 'email', 'chat', 'whatsapp', etc.
  displayName       String
  type              ChannelType
  category          ChannelCategory
  
  // Channel configuration
  isActive          Boolean  @default(true)
  isEnabled         Boolean  @default(true)
  requiresAuth      Boolean  @default(false)
  supportsRealTime  Boolean  @default(false)
  supportsFiles     Boolean  @default(false)
  maxFileSize       Int?     // Maximum file size in bytes
  supportedFileTypes String[] @default([])
  
  // API configuration
  apiEndpoint       String?  // External API endpoint
  apiKey            String?  // Encrypted API key
  webhookUrl        String?  // Webhook for incoming messages
  rateLimit         Int?     // Messages per minute
  timeout           Int?     // Timeout in seconds
  
  // Healthcare compliance
  hipaaCompliant    Boolean  @default(false)
  requiresConsent   Boolean  @default(true)
  dataRetention     Int?     // Days to retain messages
  encryptionRequired Boolean @default(false)
  
  // Integration settings
  syncEnabled       Boolean  @default(true)
  autoResponse      Boolean  @default(true)
  intelligentRouting Boolean @default(true)
  
  // UI configuration
  icon              String?  // Icon for UI
  color             String?  // Brand color
  description       String?  @db.Text
  
  // Performance metrics
  messageCount      Int      @default(0)
  avgResponseTime   Int?     // Average response time in seconds
  successRate       Float?   // Success rate percentage
  
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt

  // Relationships
  messages          ChannelMessage[]
  conversations     ChannelConversation[]
  integrations      ChannelIntegration[]
  rules             RoutingRule[]

  @@index([type])
  @@index([isActive])
  @@index([category])
  @@map("communication_channels")
}

// Channel Integration Management
model ChannelIntegration {
  id                String   @id @default(cuid())
  channelId         String
  name              String
  type              IntegrationType
  
  // Integration details
  provider          String   // 'twilio', 'whatsapp', 'telegram', etc.
  providerConfig    Json     @default("{}")
  credentials       Json     @default("{}") // Encrypted credentials
  
  // Configuration
  isActive          Boolean  @default(true)
  autoStart         Boolean  @default(false)
  webhooksEnabled   Boolean  @default(true)
  
  // Health monitoring
  lastHealthCheck   DateTime?
  healthStatus      IntegrationHealth @default(UNKNOWN)
  errorCount        Int      @default(0)
  uptimePercentage  Float?   // Uptime percentage
  
  // Usage statistics
  messagesProcessed Int      @default(0)
  lastMessageAt     DateTime?
  
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt

  channel           CommunicationChannel @relation(fields: [channelId], references: [id], onDelete: Cascade)

  @@index([channelId])
  @@index([type])
  @@index([isActive])
  @@map("channel_integrations")
}

// Unified Conversation Management
model ChannelConversation {
  id                String   @id @default(cuid())
  channelId         String
  
  // Conversation identification
  conversationId    String   @unique // External conversation ID
  subject           String?
  priority          ConversationPriority @default(NORMAL)
  
  // Participants
  customerId        String?  // Customer/user ID
  customerName      String
  customerEmail     String?
  customerPhone     String?
  agentId           String?  // Assigned agent
  agentName         String?
  
  // Conversation state
  status            ConversationStatus @default(ACTIVE)
  subStatus         String?  // Detailed status
  isClosed          Boolean  @default(false)
  isArchived        Boolean  @default(false)
  
  // Healthcare context
  clinicId          String?  // Related clinic
  doctorId          String?  // Related doctor
  serviceId         String?  // Related service
  patientId         String?  // Related patient
  appointmentId     String?  // Related appointment
  
  // Communication preferences
  preferredChannel  String?  // Preferred communication method
  language          String   @default("en")
  timezone          String?  // Customer timezone
  businessHours     Boolean  @default(false)
  
  // SLA tracking
  slaTarget         Int?     // Target response time in minutes
  slaBreached       Boolean  @default(false)
  firstResponseAt   DateTime?
  lastResponseAt    DateTime?
  resolvedAt        DateTime?
  
  // Analytics
  messageCount      Int      @default(0)
  agentResponseCount Int     @default(0)
  customerResponseCount Int  @default(0)
  escalationCount   Int      @default(0)
  
  // Integration data
  externalRefs      Json     @default("{}") // External system references
  metadata          Json     @default("{}")
  
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
  lastMessageAt     DateTime @default(now())
  closedAt          DateTime?

  channel           CommunicationChannel @relation(fields: [channelId], references: [id], onDelete: Cascade)

  @@index([channelId])
  @@index([customerId])
  @@index([status])
  @@index([priority])
  @@index([createdAt])
  @@index([lastMessageAt])
  @@map("channel_conversations")
}

// Channel Message Management
model ChannelMessage {
  id                String   @id @default(cuid())
  channelId         String
  conversationId    String?
  
  // Message identification
  messageId         String   @unique // External message ID
  threadId          String?  // Thread/conversation ID
  
  // Content
  content           String   @db.Text
  messageType       MessageType
  mediaType         MediaType?
  
  // Participants
  senderId          String?  // Sender user ID
  senderName        String
  senderType        MessageSenderType
  senderChannel     String   // Channel from which message was sent
  recipientId       String?  // Recipient user ID
  recipientName     String?
  recipientType     MessageRecipientType?
  
  // Message state
  status            MessageStatus @default(PENDING)
  deliveryStatus    DeliveryStatus @default(PENDING)
  
  // Timestamps
  sentAt            DateTime @default(now())
  deliveredAt       DateTime?
  readAt            DateTime?
  processedAt       DateTime?
  
  // Healthcare context
  clinicId          String?  // Related clinic
  doctorId          String?  // Related doctor
  serviceId         String?  // Related service
  patientId         String?  // Related patient
  
  // Message features
  hasAttachments    Boolean  @default(false)
  attachmentCount   Int      @default(0)
  attachments       Json     @default("[]")
  
  // AI and automation
  sentiment         MessageSentiment @default(NEUTRAL)
  intent            MessageIntent?
  confidence        Float?   // AI confidence score
  requiresResponse  Boolean  @default(false)
  autoResponseSent  Boolean  @default(false)
  
  // Voice-to-text
  isVoiceMessage    Boolean  @default(false)
  transcript        String?  @db.Text
  audioDuration     Int?     // Duration in seconds
  speechConfidence  Float?   // Speech recognition confidence
  
  // Processing
  processed         Boolean  @default(false)
  processingNotes   String?  @db.Text
  errorMessage      String?  @db.Text
  
  // Metadata
  metadata          Json     @default("{}")
  tags              String[] @default([])
  
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt

  channel           CommunicationChannel @relation(fields: [channelId], references: [id], onDelete: Cascade)
  conversation      ChannelConversation? @relation(fields: [conversationId], references: [id], onDelete: SetNull)

  @@index([channelId])
  @@index([conversationId])
  @@index([senderId])
  @@index([messageId])
  @@index([status])
  @@index([sentAt])
  @@index([sentiment])
  @@map("channel_messages")
}

// Chatbot Configuration and Management
